#include<stdio.h>
#include<conio.h>
int main()
{
    int Number=0,Temp=0,Cnt=0;

    printf("\n Enter a Number To Find Its Count : ");
    scanf("%d",&Number);
    for(Temp=Number;Temp>0;Temp=Temp/10,Cnt++);
    printf("\n Given Number Of %d And its count Is %d",Number,Cnt);
    getch();
    return 0;
}

